<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!--inherit contact us template for change designing-->
    <template id="inherit_website_contactus_custom" inherit_id="website.contactus">
        <xpath expr="//div[hasclass('oe_structure')]" position="replace">

            <div id="wrap" class="oe_structure oe_empty container sky_main">
                <div class="row">
                    <div class="oe_structure oe_empty"/>
                    <div class="col-md-12">
                        <img class="sky_header_class header-section"
                             src="/sky_website/static/src/img/Photo.png"/>
                    </div>
                    <div class="oe_structure oe_empty"/>
                    <div class="col-md-6">
                        <p class="contact_us_main_text_class">Contact Us</p>
                        <p class="contact_us_text_class">
                            Weâ€™d Love To Hear From You
                            If you are curious about any of our services, you can always give us a call or drop a mail -
                            we are
                            always ready to clear out the answers to all your questions.
                        </p>
                    </div>
                    <div class="col-md-6">
                    </div>
                </div>
                <div class="oe_structure oe_empty"/>
                <div class="row mt-5">
                    <div class="col-lg-4 mt-4 mt-lg-0">
                        <ul class="list-unstyled mb- ps-2">
                            <li class="mt-5">
                                <img class="fa fa-fw me-3" style="font-size:18px;margin-left:8px;"
                                     src="/sky_website/static/src/img/icon-map.png"/>
                                <storng style="font-family: GT Walsheim Pro;font-weight: 700;">
                                    Skyscend Business Solutions Pvt. Ltd.
                                </storng>
                                <p style="margin-left:52px;color: #776C82;">
                                    A/301, Shreedhar Aurum, Nr. Shivalaya Parisar<br/>Kudasan<br/>Gandhinagar 382421
                                    <br/>Gujarat GJ<br/>India
                                </p>
                            </li>
                            <li class="mt-3">
                                <i class="fa fa-envelope-o  fa-fw me-3" style="color: red;font-size:28px;"/>
                                <span style="margin-top:-10px;">
                                    <storng style="font-family: GT Walsheim Pro;font-weight: 700;">
                                        info@skyscendbs.com
                                    </storng>
                                </span>
                            </li>
                            <li class="mt-3">
                                <i class="fa fa-mobile-phone fa-fw me-2" style="color: red;font-size:32px;"/>

                                <storng style="font-family: GT Walsheim Pro;font-weight: 700;">
                                    +91-9909453634
                                    <br/>
                                </storng>
                                <storng style="font-family: GT Walsheim Pro;font-weight: 700;margin-left:53px;">
                                    +91-9725251544
                                </storng>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="oe_structure oe_empty"/>
                <section class="s_website_form" data-vcss="001" data-snippet="s_website_form">
                    <div class="container">
                        <div class="map-contact-us col-md-12"/>
                        <form id="contactus_form" action="/website/form/" method="post"
                              enctype="multipart/form-data" class="o_mark_required p-5 mt-3 mb-5" data-mark="*"
                              data-model_name="mail.mail" data-success-mode="redirect"
                              data-success-page="/contactus-thank-you" data-pre-fill="true">
                            <div class="form-group row p-5"
                                 style="background: #0C6D9E;color: #FCFCFC;border-radius: 6px;">
                                <div class="col-sm-6 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px" for="contact1">
                                        <span class="s_website_form_label_content">YOUR NAME</span>
                                    </label>
                                    <div class="col-sm">
                                        <input id="contact1" type="text" placeholder="Your Name"
                                               style="height: 54px;"
                                               class="form-control s_website_form_input" name="name"
                                               required="" data-fill-with="name"/>
                                    </div>
                                </div>
                                <div class="col-sm-6 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px" for="contact2">
                                        <span class="s_website_form_label_content">PHONE NUMBER
                                        </span>
                                    </label>
                                    <div class="col-sm">
                                        <input id="contact2" type="tel" placeholder="Phone Number"
                                               style="height: 54px;"
                                               class="form-control s_website_form_input"
                                               name="phone" data-fill-with="phone"/>
                                    </div>
                                </div>
                                <div class="col-sm-6 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px" for="contact3">
                                        <span class="s_website_form_label_content">YOUR EMAIL</span>
                                    </label>
                                    <div class="col-sm">
                                        <input id="contact3" type="email" placeholder="Email" style="height: 54px;"
                                               class="form-control s_website_form_input"
                                               name="email_from" required=""
                                               data-fill-with="email"/>
                                    </div>
                                </div>
                                <div class="col-sm-6 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px" for="contact4">
                                        <span class="s_website_form_label_content">YOUR COMPANY</span>
                                    </label>
                                    <div class="col-sm">
                                        <input id="contact4" type="text" placeholder="Company Name"
                                               style="height: 54px;"
                                               class="form-control s_website_form_input"
                                               name="company"
                                               data-fill-with="commercial_company_name"/>
                                    </div>
                                </div>
                                <div class="col-sm-12 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px" for="contact5">
                                        <span class="s_website_form_label_content">SUBJECT</span>
                                    </label>
                                    <div class="col-sm">
                                        <input id="contact5" type="text" placeholder="Write your subject"
                                               style="height: 54px;"
                                               class="form-control s_website_form_input"
                                               name="subject" required=""/>
                                    </div>
                                </div>
                                <div class="col-sm-12 p-3">
                                    <label class="col-form-label col-sm-auto s_website_form_label"
                                           style="width: 200px; height" for="contact6">
                                        <span class="s_website_form_label_content">YOUR MESSAGE</span>
                                    </label>
                                    <div class="col-sm">
                                        <textarea id="contact6" placeholder="Write your message"
                                                  style="height: 234px;"
                                                  class="form-control s_website_form_input"
                                                  name="description" required=""/>
                                    </div>
                                </div>
                                <div class="col-sm-5 mt-5"/>
                                <div class="col-sm-4 mt-5 s_website_form_submit" data-name="Submit Button">
                                    <div style="width: 200px;" class="s_website_form_label"/>
                                    <a href="#" role="button" class="s_website_form_send">
                                        <button type="button" class="btn mb-2 send_button_class">
                                            <span style="color: white;font-size: 12px;letter-spacing: 1px;font-weight: 700;">
                                                SEND MESSAGE
                                            </span>
                                        </button>
                                    </a>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="oe_structure oe_empty"/>
                </section>
            </div>
            <div class="oe_structure oe_empty"/>
        </xpath>
    </template>
</odoo>
